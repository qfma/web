// A style to set the fullscreen background image for all pages
// Has to be an erb file to import the asset_path
body { 
  background: url(<%= asset_path "qfma_background_2.jpg" %>) no-repeat center center fixed; 
  // background: white;
  -webkit-background-size: cover;
  -moz-background-size: cover;
  -o-background-size: cover;
  background-size: cover;
}